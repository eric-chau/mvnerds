			<!-- CREATE ITEM BUILD -->
			<trans-unit id="0">
				<source>create_item_build.title</source>
				<target>Customize a recommended items list for League of Legends</target>
			</trans-unit>
			<trans-unit id="00">
				<source>item_popover_cost</source>
				<target>Cost</target>
			</trans-unit>
			<trans-unit id="1">
				<source>create_item_build.h1</source>
				<target>Customize a recommended items list</target>
			</trans-unit>
			<trans-unit id="2">
				<source>create_item_build.p.1</source>
				<target>
					Welcome! Follow the steps to create a new recommended items list for your favourite champions. This list will be displayed in your League of Legends game.
				</target>
			</trans-unit>
			
			
			<trans-unit id="3">
				<source>create_item_build.h2.1</source>
				<target>Step 1 : select one or many champions</target>
			</trans-unit>
			<trans-unit id="4">
				<source>create_item_build.p.2</source>
				<target>The new recommended items list will be set for each of the selected champions</target>
			</trans-unit>
			<trans-unit id="5">
				<source>create_item_build.tip.1</source>
				<target>
					Tip : you can filter the champions by category and then select all of them to change their recommended items list all at once.
				</target>
			</trans-unit>
			
			
			<trans-unit id="6">
				<source>create_item_build.h2.2</source>
				<target>Step 2 : select the game mode</target>
			</trans-unit>
			<trans-unit id="7">
				<source>create_item_build.p.3</source>
				<target>
					Depending on the League of Legends game mode, the recommended items list for your champions will be different.
				</target>
			</trans-unit>
			<trans-unit id="8">
				<source>create_item_build.tip.2</source>
				<target>
					Note : Only the items authorised in the game mode selected will appear in the list.
				</target>
			</trans-unit>
			
			
			<trans-unit id="9">
				<source>create_item_build.h2.3</source>
				<target>Step 3 : select your items</target>
			</trans-unit>
			<trans-unit id="10">
				<source>create_item_build.p.4</source>
				<target>
					You must now select the items that will form your list. Drag and drop those items into the blocks placed on the right of your screen.
					You can create many blocks, rename them, delete them, or sort them.
				</target>
			</trans-unit>
			<trans-unit id="11">
				<source>create_item_build.tip.3</source>
				<target>
					Tip : use the filtering system to find easily your items.
				</target>
			</trans-unit>
			<trans-unit id="12">
				<source>create_item_build.tip.4</source>
				<target>
					Note : Blocks left empty won't be used for the recommended items list.
				</target>
			</trans-unit>
			
			<trans-unit id="13">
				<source>create_item_build.h2.4</source>
				<target>Step 4 : name your list</target>
			</trans-unit>
			<trans-unit id="14">
				<source>create_item_build.p.5</source>
				<target>
					You now have to name your recommended items list. If your are registered on mvnerds.com you will be able to save your list and download it again in the future or edit it.
				</target>
			</trans-unit>
			<trans-unit id="014">
				<source>create_item_build.list_name.placeholder</source>
				<target>Name of the list...
				</target>
			</trans-unit>
			
			<trans-unit id="15">
				<source>create_item_build.h2.5</source>
				<target>Last step</target>
			</trans-unit>
			<trans-unit id="16">
				<source>create_item_build.p.6</source>
				<target>
					Congratulations, you just made a new recommended items list.
					You can now "Save and Generate" or "Generate only".
				</target>
			</trans-unit>
			<trans-unit id="016">
				<source>create_item_build.tip.5</source>
				<target>
					Note : If you use "Generate Only" the build will not be saved but you will be able to downloaded it anyway.
				</target>
			</trans-unit>
			
			<trans-unit id="17">
				<source>create_item_build.max_builds_reached</source>
				<target>
					You can't save more than 7 builds.
				</target>
			</trans-unit>
			
			<trans-unit id="18">
				<source>create_item_build.btn.save_and_generate</source>
				<target>
					Save and Generate
				</target>
			</trans-unit>
			<trans-unit id="19">
				<source>create_item_build.btn.generate_only</source>
				<target>
					Generate Only
				</target>
			</trans-unit>
			<trans-unit id="20">
				<source>create_item_build.btn.save</source>
				<target>
					Save
				</target>
			</trans-unit>
			
			
			<trans-unit id="21">
				<source>create_item_build.modal_authenticate.h3</source>
				<target>
					Saving the build...
				</target>
			</trans-unit>
			
			<trans-unit id="22">
				<source>create_item_build.modal_authenticate.p1</source>
				<target>
					You have to be authenticated to be allowed to use the "Save" functionality. Registering or authenticating will not erase your current build, it will be stored temporarily
				</target>
			</trans-unit>
			<trans-unit id="23">
				<source>create_item_build.modal_authenticate.p2</source>
				<target>
					If you are not registered yet, you can create an accout now.
				</target>
			</trans-unit>
			<trans-unit id="24">
				<source>create_item_build.modal_authenticate.p3</source>
				<target>
					If you do not want to create an account now, you can just download your item build, however, you won't be able to edit it, download it again or delete it.
				</target>
			</trans-unit>

			<trans-unit id="25">
				<source>create_item_build.modal_authenticate.btn.cancel</source>
				<target>
					Cancel
				</target>
			</trans-unit>
			<trans-unit id="26">
				<source>create_item_build.modal_authenticate.btn.sign_in</source>
				<target>
					Sign in
				</target>
			</trans-unit>
			<trans-unit id="27">
				<source>create_item_build.modal_authenticate.btn.inscription</source>
				<target>
					Registration
				</target>
			</trans-unit>
			<trans-unit id="28">
				<source>create_item_build.modal_authenticate.btn.download_only</source>
				<target>
					Download Only
				</target>
			</trans-unit>

			<trans-unit id="29">
				<source>create_item_build.modal_dl_build.h3</source>
				<target>
					Recommended items list download
				</target>
			</trans-unit>
			
			<trans-unit id="30">
				<source>create_item_build.modal_dl_build.p1</source>
				<target>
					If your "League of Legends" directory does not figure in one of the directories present in this list :
				</target>
			</trans-unit>
			<trans-unit id="31">
				<source>create_item_build.modal_dl_build.li_info</source>
				<target>
					 | This is the directory you saved before and it will be replaced if you specify a new value below.
				</target>
			</trans-unit>
			<trans-unit id="32">
				<source>create_item_build.modal_dl_build.p2</source>
				<target>
					Please enter yours here : 
				</target>
			</trans-unit>
			
			<trans-unit id="33">
				<source>create_item_build.modal_dl_build.btn.cancel</source>
				<target>
					Cancel
				</target>
			</trans-unit>
			<trans-unit id="34">
				<source>create_item_build.modal_dl_build.btn.download</source>
				<target>
					Download
				</target>
			</trans-unit>
			<trans-unit id="35">
				<source>create_item_build.modal_dl_build.input.placeholder</source>
				<target>ex : D:/Riot/</target>
			</trans-unit>

			<trans-unit id="36">
