<div class="vote-block row-fluid">
	<div class="span5">
		<div class="row-fluid like-block">
			<a href="#" data-like="true" class="btn-vote-like btn-vote {% if not can_like %}disabled{% endif %}"><i class="icon-thumbs-up"></i></a>
		</div>
		<div class="row-fluid dislike-block">
			<a href="#" data-like="false" class="btn-vote-dislike btn-vote {% if not can_dislike %}disabled{% endif %}"><i class="icon-thumbs-down"></i></a>
		</div>
	</div>
	<div class="span7">
		<div class="row-fluid percentage-vote-block">
			like : <span class="like_count">{{likes_count}}</span> | dislike :  <span class="dislike_count">{{dislikes_count}}</span>
		</div>
		<div class="row-fluid total-vote-block">
			total : <span class="vote_count">{{votes_count}}</span>
		</div>
	</div>
</div>

<script type="text/javascript">
	var objectSlug = '{{ object_slug }}',
		objectType = '{{ object_type }}';
</script>
<script type="text/javascript" src="{{ asset('medias/js/vote/vote.js') }}"></script>