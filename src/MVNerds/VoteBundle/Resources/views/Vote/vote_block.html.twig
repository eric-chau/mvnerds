<div class="vote-block row-fluid">
	<div class="span5">
		<div class="row-fluid like-block">
			<a href="#" class="btn-vote-like"><i class="icon-thumbs-up"></i></a>
		</div>
		<div class="row-fluid dislike-block">
			<a href="#" class="btn-vote-dislike"><i class="icon-thumbs-down"></i></a>
		</div>
	</div>
	<div class="span7">
		<div class="row-fluid percentage-vote-block">
			like : {{likes_count}} | dislike :  {{dislikes_count}}
		</div>
		<div class="row-fluid total-vote-block">
			total : {{total_votes_count}}
		</div>
	</div>
</div>