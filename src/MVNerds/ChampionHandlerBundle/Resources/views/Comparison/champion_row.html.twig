{% if champion.getId() != reference_champion.getId() %}
	<tr>
		<td>
			<img src="{{ asset('/images/champions/square/' ~ champion.getSlug() ~ '.png') }}" class="tooltip-anchor" rel="tooltip" data-placement="bottom" data-original-title="{{ champion.getName() }}" />
		</td>
		{% set diff = champion.getBaseHealth() - reference_champion.getBaseHealth() %}
		{% if diff|length > 9 %}
			{% set diff = diff|number_format(3) %}
		{% endif %}
		<td>
			<span class="diff {% if diff > 0 %}plus{% else %}minus{% endif %}">{{ diff }}</span>
			<span class="own">{{ champion.getBaseHealth() }}</span>
		</td>

		{% set diff = champion.getBaseHealthRegen() * 5 - reference_champion.getBaseHealthRegen() * 5 %}
		{% if diff|length > 9 %}
			{% set diff = diff|number_format(3) %}
		{% endif %}
		<td>
			<span class="diff {% if diff > 0 %}plus{% else %}minus{% endif %}">{{ diff }}</span>
			<span class="own">{{ champion.getBaseHealthRegen() * 5 }}</span>
		</td>

