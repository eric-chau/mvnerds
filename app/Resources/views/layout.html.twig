<!DOCTYPE html>
	<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7" xmlns:fb="http://ogp.me/ns/fb#"> <![endif]-->
	<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8" xmlns:fb="http://ogp.me/ns/fb#"> <![endif]-->
	<!--[if IE 8]>         <html class="no-js lt-ie9" xmlns:fb="http://ogp.me/ns/fb#"> <![endif]-->
	<!--[if gt IE 8]><!--> <html class="no-js" xmlns:fb="http://ogp.me/ns/fb#"> <!--<![endif]-->
	<head>
		<meta charset="utf-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
		<title>{% block title %}Guides, Builds and Tools for League of Legends - LoL | MVNerds{% endblock %}</title>
		<meta name="description" content="{{'Overview.content.p1'|trans}}">
		<meta name="viewport" content="width=device-width">
		{% block metas %}{% endblock %}
		
		<link href='http://fonts.googleapis.com/css?family=Anaheim' rel='stylesheet' type='text/css'>
		<link href='http://fonts.googleapis.com/css?family=Source+Sans+Pro' rel='stylesheet' type='text/css'>
		<link href='http://fonts.googleapis.com/css?family=Cabin' rel='stylesheet' type='text/css'>
		
		{% if app.environment == 'dev' %}
			<link href='{{ asset('medias/css/bootstrap.css') }}' rel='stylesheet' type='text/css'>
			<link href='{{ asset('medias/css/mvnerds-bootstrap.css') }}' rel='stylesheet' type='text/css'>
		{% endif %}
					
		{% block css %}{% endblock %}
		
		<link rel="icon" type="image/x-icon" href="{{ asset('favicon.png') }}" />
		
		<script type="text/javascript">var locale = '{{ app.request.getLocale() }}';</script>
		<script type="text/javascript" src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
		<script type="text/javascript" src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>
		
		{% if app.environment == 'dev' %}
			<script src="{{ asset('medias/js/jquery-1.8.3.min.js') }}" type="text/javascript"></script>
		{% endif %}
		
	</head>
	<body>
		<!--[if IE]>
			<p class="chromeframe" style="position: fixed;top:0; left:0;z-index:2000; width: 100%;margin:0;padding:0">{{'Comment_ie.1'|trans}}<a href="http://browsehappy.com/">{{'Comment_ie.2'|trans}}</a> {{'General.or'|trans}} <a href="http://www.google.com/chromeframe/?redirect=true">{{'Comment_ie.3'|trans}}</a> {{'Comment_ie.4'|trans}}</p>
		<![endif]-->
		{% set locale = app.request.getLocale() %}
		
		{% block body %}{% endblock %}
		
		{% if app.environment == 'dev' %}
			<script src="{{ asset('medias/js/jquery-1.8.3.min.js') }}" type="text/javascript"></script>
			<script src="{{ asset('medias/js/bootstrap/bootstrap-dropdown.js') }}" type="text/javascript"></script>
			<script src="{{ asset('medias/js/mvnerds-bootstrap.js') }}" type="text/javascript"></script>
		{% endif %}
		
		{% block javascripts %}{% endblock %}
	</body>
</html>